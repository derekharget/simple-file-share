{
  "name": "sendfile",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "npm run build && nodemon dist/index.js",
    "debug": "npm run build && nodemon dist/index.js --inspect",
    "production": "npm run build && nodemon dist/index.js",
    "prettify": "npm run prettify:src && npm run prettify:configs",
    "prettify:src": "prettier --write \"./src/**/*.{js,ts,json,d.ts}\"",
    "prettify:configs": "prettier --write \"./*.{js,ts,json,yaml,md}\"",
    "lint": "eslint --ext .js,.ts ./src",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "pre": "npm run lint && npm run prettify",
    "clean": "npm run clean:nodemodules && npm run clean:dist",
    "clean:nodemodules": "rm -rf ./node_modules",
    "clean:dist": "rm -rf ./dist",
    "clean:package": "rm -rf ./package-lock.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/eslint": "^8.4.5",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/node": "^18.6.1",
    "@types/prettier": "^2.6.3",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^5.1.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "stylelint-prettier": "^2.0.0",
    "ts-node": "^10.9.1",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "xss": "^1.0.13"
  }
}
